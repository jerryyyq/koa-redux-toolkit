{
  "name": "koa-redux-toolkit",
  "version": "1.0.0",
  "description": "koa mysql react redux toolkit",
  "main": "server.js",
  "keywords": [
    "koa",
    "mysql",
    "redux",
    "react",
    "ES6"
  ],
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "babel-cli": "^6.14.0",
    "ip": "^1.1.3",
    "koa": "^1.2.1",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "mysql-co": "0.0.4"
  },
  "devDependencies": {
    "antd": "^1.11.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "redux": "^3.5.2",
    "redux-devtools": "^3.3.1",
    "sassify": "^2.0.0",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "test": "node --harmony server.js",
    "build": "./node_modules/.bin/browserify --debug -t sassify --extension=js -o client/bundle.js client/main.js",
    "build-continue": "./node_modules/.bin/watchify --debug -t sassify --extension=js -o client/bundle.js client/main.js",
    "start": "./node_modules/.bin/babel-node --debug server.js"
  },
  "author": "yangyuqi",
  "license": "ISC"
}
